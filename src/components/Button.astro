---
const {
  color = "green",
  url = "",
  newtab = false,
  As = "a",
  class: className = "",
  ...props
} = Astro.props;
---

<As
        class={`not-prose button button-${color} ${className}`}
	href={url}
	target={newtab ? "_blank" : "_self"}
	rel={newtab ? "noopener noreferrer" : ""}
	{...props}
>
	<slot />
</As>
